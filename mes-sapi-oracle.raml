  merchantAccountEquipment: !include dataType/merchantAccountEquipment.raml
  merchantAccountEquipmentEquipmentLineItems: !include dataType/merchantAccountEquipmentEquipmentLineItems.raml
  merchantAccountEquipmentTerminalFeature: !include dataType/merchantAccountEquipmentTerminalFeature.raml

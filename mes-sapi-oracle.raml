/views:
  /{viewName}:
    is: [queryable, error_response]
    displayName: Get View Data
    description: This is used to get data from an oracle view.
    get:
      description:  Get view records based on the criteria provided.
      responses:
        200:
          body:
            application/json:
            
/tables:
  /{tableName}:
    is: [queryable, error_response]
    displayName: Get Table Data
    description: This is used to get data from an oracle table.
    get:
      description:  Get table records based on the criteria provided.
      responses:
        200:
          body:
            application/json:

/application:
  post:
    displayName: Create New Merchant in Oracle
    body:
      application/json:
        description: The new merchant account JSON you want to post

    responses:
      200:
        description: Create a new merchant account
        body:
          application/json:
            type: merchantAccountResponse
      201:
        description: Created
        body:
          application/json:
            type: merchantAccountResponse
    
/mesGetTestRecord/{appseqnum}:
  get:
    displayName: Get all inserted records as a package, by using unique appseqnum
    responses:
      200:
        description: Inserted package
        body:
          application/json:
            type: merchantAccount
            
/mesDeleteTestRecord/{appseqnum}:
  delete:
    displayName: Delete all inserted records from all tables, by using unique appseqnum
    responses:
      200:
        description: Deleted rows
        body:
          application/json:

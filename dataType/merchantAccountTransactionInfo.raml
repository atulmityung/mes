  tranchrgAvsFee?:
    maximum: 99.999
    type: number
    format: double
  tranchrgVoiceFee:
    maximum: 99.999
    type: number
    format: double
  tranchrgMminChrg:
    maximum: 999.99
    type: number
    format: double
  tranchrgPerAuth:
    maximum: 99.99
    type: number
    format: double

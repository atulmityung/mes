      - jcb
      - ebt
      - discover
      - debitWithPin
      - amexPassThroughRate
      - amexBundleRate
      - amexSeDirectRate
      - vmcPassThroughRate
      - vmcCreditQualifiedRate
      - vmcDebitQualifiedRate
      - visaCreditDiscountRate
      - visaCheckDiscountRate
      - visaBusinessDiscountRate
      - masterCardCreditDiscountRate
      - masterCardCheckDiscountRate
      - masterCardBusinessDiscountRate
